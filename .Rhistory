annotated_powerlaw_graph <- function(V, E, alpha) {
g <- sample_fitness_pl(V, E, alpha)
g$mean_degree     <- mean(degree(g))
g$mean_distance   <- mean_distance(g)
g$transitivity    <- transitivity(g)
g$components      <- components(g)
return(g)
}
